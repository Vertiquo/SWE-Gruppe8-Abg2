{
  "name": "acme",
  "version": "1.0.0",
  "description": "Beispiel",
  "author": "Juergen Zimmermann",
  "private": true,
  "license": "GPL-3.0",
  "scripts": {
    "ng": "ng",
    "prestart": "npm run prettier && set NG_PERSISTENT_BUILD_CACHE=1",
    "start": "ng serve --port=4200 --open=true --ssl --ssl-key=extras/webserver/private-key.pem --ssl-cert=extras/webserver/certificate.cer",
    "start:http": "ng serve --port=4200",
    "start:prod": "ng serve --port=4200 --configuration production",
    "start:de": "ng serve --configuration=de --port=4200 --open=true --ssl --ssl-key=extras/webserver/private-key.pem --ssl-cert=extras/webserver/certificate.cer",
    "start:ORIG": "ng serve",
    "test": "set NODE_OPTIONS=--openssl-legacy-provider && cypress run --browser chrome",
    "build": "ng build",
    "build:dev": "ng build --configuration=development",
    "build:watch": "ng build --configuration=development --watch",
    "browserslist": "browserslist",
    "browserslist:update": "npx browserslist@latest --update-db",
    "clean": "npx rimraf dist doc",
    "cypress": "cypress open --browser chrome --e2e",
    "eslint": "npx eslint --ext .ts src",
    "eslint:cypress": "npx eslint --ext .js cypress/integration",
    "i18n": "ng xi18n --output-path=src/locale --out-file=messages.de.xlf",
    "leasot": "npx leasot --exit-nicely src/**/*.ts",
    "lighthouse:hs": "lighthouse https://www.h-ka.de/home/ --view --output-path=temp/www.h-ka.de.html",
    "lighthouse:hskampus": "lighthouse http://www.iwi.h-ka.de/hskampus-web/ --view --output-path=temp/app.hskampus.de.html --chrome-flags='--disable-web-security'",
    "local-web-server": "ws",
    "prepack": "npm run build:dev",
    "pack": "pack build juergenzimmermann/buchweb:1.0.0 --builder paketobuildpacks/builder:base",
    "prettier": "npx prettier --write src/**/*.ts src/**/*.html src/**/*.scss",
    "presource-map-explorer": "npm run build -- --source-map && del dist/acme/ngsw-worker.js && del dist/acme/safety-worker.js && del dist/acme/worker-basic.min.js",
    "source-map-explorer": "npx source-map-explorer dist/acme/main.*.js dist/acme/main.*.js.map --html source-map-explorer.html --only-mapped",
    "stylelint": "npx stylelint src/app/**/*.scss",
    "typedoc": "npx typedoc --verbose --options typedoc.json src",
    "webhint": "npx hint src",
    "prewebpack-bundle-analyzer": "npm run build -- --stats-json",
    "webpack-bundle-analyzer": "npx webpack-bundle-analyzer dist/acme/stats.json dist/acme --port 9999"
  },
  "dependencies": {
    "@angular/animations": "15.0.0",
    "@angular/common": "15.0.0",
    "@angular/compiler": "15.0.0",
    "@angular/core": "15.0.0",
    "@angular/forms": "15.0.0",
    "@angular/localize": "15.0.0",
    "@angular/platform-browser": "15.0.0",
    "@angular/platform-browser-dynamic": "15.0.0",
    "@angular/pwa": "15.0.0",
    "@angular/router": "15.0.0",
    "@angular/service-worker": "15.0.0",
    "@js-temporal/polyfill": "0.4.3",
    "@popperjs/core": "2.11.6",
    "@swimlane/ngx-charts": "20.1.0",
    "angular-calendar": "0.30.1",
    "bootstrap": "5.2.2",
    "date-fns": "2.29.3",
    "loglevel": "1.8.1",
    "rxjs": "7.5.7",
    "tslib": "2.4.1",
    "zone.js": "0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.0",
    "@angular-eslint/builder": "15.0.0",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/schematics": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "15.0.0",
    "@angular/compiler-cli": "15.0.0",
    "@angular/language-service": "15.0.0",
    "@types/d3-scale": "4.0.2",
    "@types/d3-selection": "3.0.3",
    "@types/node": "18.11.9",
    "@types/web": "0.0.79",
    "@typescript-eslint/eslint-plugin": "5.43.0",
    "@typescript-eslint/parser": "5.43.0",
    "cypress": "11.1.0",
    "eslint": "8.27.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-eslint-comments": "3.2.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-regexp": "1.10.0",
    "eslint-plugin-rxjs": "5.0.2",
    "eslint-plugin-unicorn": "44.0.2",
    "hint": "7.1.3",
    "leasot": "13.2.0",
    "lighthouse": "9.6.8",
    "local-web-server": "5.2.1",
    "lws-redirect": "2.1.2",
    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "source-map-explorer": "2.5.3",
    "stylelint": "14.15.0",
    "stylelint-config-sass-guidelines": "9.0.1",
    "stylelint-config-standard": "29.0.0",
    "typescript": "4.8.4",
    "webpack-bundle-analyzer": "4.7.0"
  }
}
